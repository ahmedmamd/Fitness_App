<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitness">

    <uses-permission android:name="android.gms.permission.ACTIVITY_RECOGNITION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fitness">
        <activity android:name=".ui.Sports"></activity>
        <activity android:name=".ui.advanced.StomachDaysAdvanced" />
        <activity android:name=".ui.advanced.ShoulderDaysAdvanced" />
        <activity android:name=".ui.advanced.LegsDaysAdvanced" />
        <activity android:name=".ui.advanced.ArmDaysAdvanced" />
        <activity android:name=".ui.middle.StomachDaysMiddle" />
        <activity android:name=".ui.middle.ShoulderDaysMiddle" />
        <activity android:name=".ui.middle.LegsDaysMiddle" />
        <activity android:name=".ui.middle.ArmDaysMiddle" />
        <activity android:name=".ui.start.LegsDaysStart" />
        <activity android:name=".ui.start.ShoulderDaysStart" />
        <activity android:name=".ui.start.StomachDaysStart" />
        <activity android:name=".ui.start.DaysStart" />
        <activity android:name=".ui.advanced.AdvancedActivity" />
        <activity android:name=".ui.middle.MiddleActivity" />
        <activity android:name=".ui.start.StartActivity" />
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>